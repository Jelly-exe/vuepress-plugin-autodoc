{
  "name": "vuepress-plugin-autodoc",
  "homepage": "http://bprinty.github.io/vuepress-plugin-autodoc",
  "repository": "http://github.com/bprinty/vuepress-plugin-autodoc",
  "version": "0.1.0",
  "authors": [
    "Blake Printy <bprinty@gmail.com> (http://bprinty.github.io)"
  ],
  "description": "Automatic Code Documentation for VuePress",
  "license": "Apache-2.0",
  "keywords": [
    "vuepress",
    "autodoc",
    "api",
    "jsdoc"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "tests",
    "src",
    "package-lock.json"
  ],
  "scripts": {
    "clean": "rm -rf node_modules dist",
    "build": "webpack",
    "build:dev": "webpack --mode=development",
    "lint": "eslint src --fix",
    "test": "jest --config .jest.json",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand --watch",
    "docs": "vuepress build",
    "docs:dev": "vuepress dev",
    "docs:deploy": "rm -rf .vuepress/dist && vuepress build && cd .vuepress/dist && git init && git add -A && git commit -m \"vuepress-plugin-autodoc, $npm_package_version\" && git push -f git@github.com:bprinty/vuepress-plugin-autodoc.git master:gh-pages",
    "release": "git tag -f $npm_package_version && git push -f origin $npm_package_version && yarn docs:deploy && yarn publish"
  },
  "main": [
    "dist/index.min.js"
  ],
  "dependencies": {
    "jsdoc-api": "^5.0.4"
  },
  "devDependencies": {
    "markdown-it": "^10.0.0",
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "jest": "^25.1.0",
    "mocha": "^6.1.4",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.10",
    "vuepress": "^1.2.0"
  }
}
